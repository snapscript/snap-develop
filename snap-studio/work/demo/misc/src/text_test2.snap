async func foo(): Promise<Integer> {
   for(i in 0 to 10) {
      println(i);
      await i;
      println("i="+Thread.currentThread().getName());
   }
   println("done");
   return 110;
}
println("x="+Thread.currentThread().getName());
let x = foo().get();
println(x);

func do(): Yield<Integer> {
   yield 1;
}
println(Promise.class);
println(Yield.class);
let t = do().iterator().next().intValue();
println(t);