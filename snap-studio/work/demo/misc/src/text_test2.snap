

async func one<T: Number>(n: T): Promise<T> {
   if(n > 0) {
      await two<T>(n-1);
   }
   return 1;
}


async func two<T: Number>(n: T): Promise<T> {
   if(n > 0) {
      await one<T>(n-1);
   }
   return 1;
}


let promise: Promise<Integer> = one<Integer>(2000);

promise.join()
   .success(this::println)
   .failure(this::println);
