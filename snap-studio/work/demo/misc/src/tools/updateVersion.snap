File("c:/Work/development/snapscript")
   .findFiles(n -> n.name.equals("pom.xml"))
   .stream()
   .forEach(f -> {
      let t = f.readText();
      if(t.contains("<version>1.4.3</")) {
         let n = t.replace("<version>1.4.3</", "<version>1.4.4</");
         f.writeText(n);
      }
      
   });
